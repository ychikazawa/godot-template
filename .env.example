# Environment variables for Godot MCP Server
# Copy this file to .env and customize for your environment

# Path to Godot executable (required)
# Example for Windows:
# GODOT_PATH=C:\MyProgramFiles\Godot_v4.5.1\Godot_v4.5.1-stable_win64.exe
# Example for macOS:
# GODOT_PATH=/Applications/Godot.app/Contents/MacOS/Godot
# Example for Linux:
# GODOT_PATH=/usr/local/bin/godot

GODOT_PATH=

# Enable debug logging (optional)
DEBUG=true
